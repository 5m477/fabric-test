{
    "channelID": "_ch{{ item[0].channelnum|int }}",
{% if chaincode == "samplecc" %}
    "chaincodeID": "sample_cc",
{% elif chaincode == "samplejs" %}
    "chaincodeID": "sample_js",
{% endif %}    
    "chaincodeVer": "v0",
    "logLevel": "ERROR",
    "transMode": "Simple",
    "transType": "install",
    "nProcPerOrg": "4",
    "TLS": "enabled",
    "channelOpt": {
        "name":  "{{ item[0].name }}",
        "action":  "create",
        "orgName": [
            "org{{ item[1]|int }}"
        ]
    },
    "deploy": {
{% if chaincode == "samplecc" %}
        "chaincodePath": "github.com/hyperledger/fabric-test/chaincodes/samplecc/go",
{% elif chaincode == "samplejs" %}
        "chaincodePath": "github.com/hyperledger/fabric-test/chaincodes/samplecc/node",
        "language": "node",
{% endif %}
        "fcn": "init",
        "args": []
    },
    "SCFile": [
                {"ServiceCredentials":"CITest/CISCFiles/config-chan{{ item[0].channelnum|int }}-TLS.json"}
	]
}
